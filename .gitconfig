[remote "mwouts"]
    url = git@github.com:mwouts/jupytext.git
    fetch = +refs/heads/*:refs/remotes/mwouts/*
[remote "origin"]
    url = git@github.com:huangyingw/mwouts_jupytext.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = mwouts
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = notebook.ipynb
    ignore = test.sh
[deploy]
	host = localhost
	path = ~/myproject/git/AI/mwouts/jupytext
